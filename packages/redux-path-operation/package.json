{
  "name": "redux-po",
  "version": "0.1.1",
  "description": "",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/panzer-vor/redux-po.git"
  },
  "main": "lib/index.js",
  "author": "<Yorke>",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "tsc --module commonjs",
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "testRegex": "(/test/.*\\.(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}",
      "src/**/*.{js,ts}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/boot.ts"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^10.11.0",
    "@types/redux": "^3.6.0",
    "jest": "^24.8.0",
    "jest-config": "^24.8.0",
    "path-operation": "^0.2.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "tslib": "^1.10.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "typedoc": "^0.12.0",
    "typescript": "^3.7.3"
  }
}
